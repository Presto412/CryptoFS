{
  "name": "src",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./src/bin/www",
    "prettify": "prettier --write src/**/*.js",
    "lint": "eslint \"src/**/*.js\""
  },
  "nodemonConfig": {
    "ignore": [
      "data/*.json",
      "data/*"
    ]
  },
  "dependencies": {
    "cookie-parser": "~1.4.3",
    "crypto-js": "^4.0.0",
    "debug": "~2.6.9",
    "dotenv": "^7.0.0",
    "ejs": "~2.5.7",
    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "js-sha1": "^0.6.0",
    "mongoose": "^5.7.5",
    "mongoose-encryption": "^2.0.2",
    "morgan": "~1.9.0",
    "multer": "^1.4.1",
    "node-forge": "^0.10.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.4"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "babel-loader": "^8.3.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.0",
    "nodemon": "^2.0.5",
    "prettier": "^2.0.5",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
